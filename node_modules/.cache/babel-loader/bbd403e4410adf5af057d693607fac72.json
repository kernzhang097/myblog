A;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA,OADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;;AAIA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,UAAA;AACA,SANA,MAMA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA,OADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;AAIA;AACA,OAbA;AAcA,KAvBA;AAwBA,IAAA,iBAxBA,6BAwBA,EAxBA,EAwBA;AAAA;;AACA,UAAA,OAAA,GAAA,YAAA,CAAA,OAAA,CAAA,UAAA,CAAA;AACA,UAAA,MAAA,GAAA;AACA,QAAA,OAAA,EAAA,OADA;AAEA,QAAA,EAAA,EAAA;AAFA,OAAA;AAIA,WAAA,MAAA,CAAA,IAAA,CAAA,qBAAA,EAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,MAAA,EAAA;AACA,cAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,QAAA,GAAA,IAAA,CAAA,QAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,OAAA,GAAA,IAAA,CAAA,OAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,SANA,MAMA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA,OADA;AAEA,YAAA,IAAA,EAAA;AAFA,WAAA;AAIA;AACA,OAbA;AAcA;AA5CA,GAvBA;AAqEA,EAAA,gBArEA,4BAqEA,EArEA,EAqEA,IArEA,EAqEA,IArEA,EAqEA;AACA,QAAA,MAAA,GAAA,MAAA,CAAA,OAAA,CACA,wDADA,CAAA;;AAGA,QAAA,MAAA,EAAA;AACA,MAAA,IAAA;AACA,KAFA,MAEA;AACA,MAAA,IAAA,CAAA,KAAA,CAAA;AACA;AACA;AA9EA,CAAA","sourcesContent":["<template>\n  <div class=\"add-post\">\n    <admin-bread-crumb :pathList=\"this.$route.matched\"></admin-bread-crumb>\n    <div class=\"container\">\n      <el-form ref=\"form\" :model=\"post\" label-width=\"80px\">\n        <el-form-item label=\"标题\">\n          <el-input v-model=\"post.title\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"描述\">\n          <el-input\n            type=\"textarea\"\n            v-model=\"post.abstract\"\n            maxlength=\"200\"\n            autosize\n            resize=\"none\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"内容\">\n          <admin-editor v-model=\"post.content\" v-if=\"flag\"></admin-editor>\n        </el-form-item>\n      </el-form>\n    </div>\n    <el-button @click=\"save\">保存</el-button>\n    <el-dialog title=\"提示\" :visible.sync=\"dialogVisible\" width=\"30%\">\n      <span>离开页面将不会保存修改</span>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"dialogVisible = false\"\n          >确 定</el-button\n        >\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport AdminBreadCrumb from '../components/AdminBreadCrumb'\nimport AdminEditor from '../components/AdminEditor'\nexport default {\n  name: 'AddPost',\n  components: {\n    AdminBreadCrumb,\n    AdminEditor\n  },\n  data() {\n    return {\n      post: {\n        title: '',\n        abstract: '',\n        content: ''\n      },\n      flag: true,\n      dialogVisible: false\n    }\n  },\n  mounted() {\n    if (this.$route.params.id) {\n      this.flag = false\n      this.getCurrentArticle(this.$route.params.id)\n    }\n  },\n  methods: {\n    save() {\n      const params = {\n        account: localStorage.getItem('username'),\n        title: this.post.title,\n        content: this.post.content,\n        abstract: this.post.abstract,\n        id: this.$route.params.id ? this.$route.params.id : ''\n      }\n      this.$axios.post('/api/saveArticle', params).then(res => {\n        if (res.data.status) {\n          this.$message({\n            message: res.data.message,\n            type: 'success'\n          })\n          this.$router.push('postList')\n        } else {\n          this.$message({\n            message: res.data.message,\n            type: 'warning'\n          })\n        }\n      })\n    },\n    getCurrentArticle(id) {\n      const account = localStorage.getItem('username')\n      const params = {\n        account: account,\n        id: id\n      }\n      this.$axios.post('/api/getCurrArticle', params).then(res => {\n        if (res.data.status) {\n          const data = res.data.data[0]\n          this.post.title = data.title\n          this.post.abstract = data.abstract\n          this.post.content = data.content\n          this.flag = true\n        } else {\n          this.$message({\n            message: res.data.message,\n            type: 'warning'\n          })\n        }\n      })\n    }\n  },\n  beforeRouteLeave(to, from, next) {\n    const answer = window.confirm(\n      'Do you really want to leave? you have unsaved changes!'\n    )\n    if (answer) {\n      next()\n    } else {\n      next(false)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.container {\n  padding-top: 40px;\n  margin-right: 40px;\n}\n</style>\n"],"sourceRoot":"src/views/admin/post"}]}