-editor @content=\"getContent\"></admin-editor>\n        </el-form-item>\n      </el-form>\n    </div>\n    <el-button @click=\"save\">保存</el-button>\n  </div>\n</template>\n\n<script>\nimport AdminBreadCrumb from '../components/AdminBreadCrumb'\nimport AdminEditor from '../components/AdminEditor'\nexport default {\n  name: 'AddPost',\n  components: {\n    AdminBreadCrumb,\n    AdminEditor\n  },\n  data() {\n    return {\n      post: {\n        title: '',\n        desc: '',\n        content: ''\n      }\n    }\n  },\n  methods:{\n      getContent(data){\n          this.post.content = data\n      },\n      save(){\n          const params = {\n              account: localStorage.getItem('username'),\n              title: this.post.title,\n              content: this.post.content,\n              desc: this.post.desc\n          }\n          this.$axios.post('/api/addArticle', params).then(res => {\n              if(res.data.status){\n                  this.$message({\n                      message: res.data.message,\n                      type: 'success'\n                  })\n                  this.$router.push('postList')\n              } else {\n                  this.$message({\n                      message: res.data.message,\n                      type: 'warning'\n                  })\n              }\n          })\n      }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.container{\n    padding-top: 40px;\n    margin-right: 40px;\n}\n</style>\n"]}]}